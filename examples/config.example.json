{
  "aws": {
    "region": "us-east-1",
    "instance_id": "i-YOUR-INSTANCE-ID-HERE",
    "profile": "default"
  },
  "server": {
    "user": "ubuntu",
    "app_path": "/home/ubuntu/YOUR-APP-NAME-HERE",
    "health_port": 8000,
    "health_path": "/health"
  },
  "monitoring": {
    "check_processes": true,
    "check_health": true,
    "check_logs": true,
    "check_syntax": false
  },
  "output": {
    "format": "text",
    "file": "monitoring_report.txt"
  }
}

